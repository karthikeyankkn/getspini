<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . index.html [L]

RewriteCond %{HTTP_HOST} ^www\.getspini\.com [NC]
RewriteRule ^(.*)$ https://www.getspini.com/$1 [L,R=301]
</IfModule>
